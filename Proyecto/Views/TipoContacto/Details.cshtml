@model Modelos.TipoContacto

@{
    ViewBag.Title = "Detalles";
}

<h2>Detalles</h2>

<div>
    <h4>Tipo de Contacto</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.Label("Nombre:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombre)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.id }, new { @class = "btn btn-default btn-primary glyphicon glyphicon-edit" })
    @Html.ActionLink("Atrás", "Index")
</p>
